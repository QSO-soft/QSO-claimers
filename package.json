{
  "name": "QSO-soft",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": "20.8.1",
    "npm": "10.1.0"
  },
  "scripts": {
    "start": "node src/scripts/runner.js",
    "build": "rimraf ./dist && tsc",
    "prepare-files": "sh prepare-files.sh",
    "==================== DEV": "====================",
    "dev": "tsc --watch",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install",
    "prettier": "prettier 'src/**/*' --write",
    "check-ts": "tsc src/**/*.ts --noEmit",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint 'src/**/*' --fix",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "==================== CLAIMER": "====================",
    "claimer:main": "node --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --loader ts-node/esm src/scripts/claimers/main.ts",
    "claimer:restart-last": "node --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --loader ts-node/esm src/scripts/claimers/restart-last.ts",
    "claimer:migrate-generate": "npm run typeorm -- -d ./src/scripts/claimers/db/index.ts migration:generate ./src/scripts/claimers/db/migrations/$npm_config_name"
  },
  "author": "QSO-soft",
  "license": "ISC",
  "dependencies": {
    "@scure/bip39": "1.2.1",
    "axios": "1.6.7",
    "ccxt": "4.0.109",
    "cloudscraper": "^4.6.0",
    "crypto-js": "4.2.0",
    "csvtojson": "2.0.10",
    "dayjs": "1.11.10",
    "ethers": "6.7.1",
    "got-scraping": "^4.0.7",
    "https-proxy-agent": "7.0.2",
    "inquirer": "9.2.11",
    "lodash": "^4.17.21",
    "moralis": "^2.26.1",
    "puppeteer": "21.3.6",
    "request": "^2.88.2",
    "socks-proxy-agent": "^8.0.4",
    "sqlite3": "^5.1.7",
    "ts-node": "10.9.1",
    "tsx": "^4.19.1",
    "typeorm": "^0.3.20",
    "typescript": "5.2.2",
    "viem": "^2.16.2",
    "web3": "4.1.2",
    "winston": "3.10.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.17.1",
    "@types/request": "^2.48.12",
    "@types/yargs": "17.0.28",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.0",
    "prettier": "3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier 'src/**/*' --write",
      "eslint 'src/**/*' --fix"
    ]
  }
}
